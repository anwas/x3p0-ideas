		"custom": {
			"elements": {
				"formInput": {
					"color": "var( --wp--preset--color-contrast )",
					"background": "var( --wp--preset--color--neutral-base )",
					"borderColor": "var( --wp--preset--color--neutral-subtle )",
					"placeholderColor": "var( --wp--preset--color--neutral-muted )"
				}
			},
			"blocks": {
				"navigation-submenu": {
					"color": "var( --wp--preset--color--contrast )",
					"background": "var( --wp--preset--color--base )",
					"border": "1px solid var( --wp--preset--color--neutral-contrast )",
					"border-radius": "4px",
					"link-color": "inherit",
					"link-background": "inherit",
					"link-hover-color": "inherit",
					"link-hover-background": "var( --wp--preset--color--neutral-base )"
				}
			}
		},
		"typography": {
			"fontFamilies": [
				{
					"fontFamily": "'Jetbrains Mono', monospace",
					"slug": "primary",
					"name": "Primary (Jetbrains Mono)",
					"fontFace": [
						{
							"fontFamily": "Jetbrains Mono",
							"fontWeight": "100 800",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"src": [ "file:./public/fonts/jetbrains-mono/jetbrains-mono.woff2" ]
						},
						{
							"fontFamily": "Jetbrains Mono",
							"fontWeight": "100 800",
							"fontStyle": "italic",
