{
	"$schema": "https://raw.githubusercontent.com/WordPress/gutenberg/trunk/schemas/json/theme.json",
	"version": 3,
	"title": "Style 2",
	"slug": "section-2",
	"blockTypes": [ "core/group", "core/query" ],
	"styles": {
		"color": {
			"background": "var:preset|color|neutral-100",
			"text": "var:preset|color|contrast"
		},
		"spacing": {
			"padding": {
				"top": "var:preset|spacing|plus-3",
				"bottom": "var:preset|spacing|plus-3",
				"left": "var:preset|spacing|plus-3",
				"right": "var:preset|spacing|plus-3"
			}
		},
		"elements": {
			"button": {
				"color": {
					"background": "var:custom|global|fill|background",
					"text": "var:custom|global|fill|color"
				},
				":focus": {
					"color": {
						"background": "var:custom|global|fill|hover-background",
						"text": "var:custom|global|fill|hover-color"
					}
				},
				":hover": {
					"color": {
						"background": "var:custom|global|fill|hover-background",
						"text": "var:custom|global|fill|hover-color"
					}
				}
			},
			"link": {
				"color": {
					"text": "var:preset|color|primary-700"
				}
			}
		},
		"blocks": {
			"core/post-title": {
				"elements": {
					"link": {
						"color": {
							"text": "inherit"
						}
					}
				}
			}
		}
	}
}
