{
	"$schema": "https://raw.githubusercontent.com/WordPress/gutenberg/trunk/schemas/json/theme.json",
	"version": 3,
	"title": "Style 1",
	"slug": "section-1",
	"blockTypes": [
		"core/column",
		"core/columns",
		"core/group",
		"core/query"
	],
	"styles": {
		"css": "& .is-style-meta, .is-style-meta a { color: inherit !important; }",
		"color": {
			"background": "var:preset|color|primary-700",
			"text": "var:preset|color|white"
		},
		"spacing": {
			"padding": {
				"top": "var:preset|spacing|plus-3",
				"bottom": "var:preset|spacing|plus-3",
				"left": "var:preset|spacing|plus-3",
				"right": "var:preset|spacing|plus-3"
			}
		},
		"elements": {
			"button": {
				"color": {
					"background": "var:preset|color|primary-900",
					"text": "var:custom|global|fill|color"
				},
				":focus": {
					"color": {
						"background": "var:preset|color|primary-950"
					}
				},
				":hover": {
					"color": {
						"background": "var:preset|color|primary-950"
					}
				}
			},
			"caption": {
				"color": {
					"text": "currentColor"
				}
			},
			"cite": {
				"color": {
					"text": "currentColor"
				}
			},
			"link": {
				"color": {
					"text": "var:preset|color|primary-50"
				},
				":focus": {
					"color": {
						"text": "currentColor"
					}
				},
				":hover": {
					"color": {
						"text": "currentColor"
					}
				}
			}
		},
		"blocks": {
			"core/post-title": {
				"elements": {
					"link": {
						"color": {
							"text": "inherit"
						}
					}
				}
			}
		}
	}
}
