{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"title": "Extant",
	"settings": {
		"blocks": {
			"core/comment-template": {
				"custom": {
					"comment": {
						"background": "transparent",
						"borderWidth": "0",
						"padding": "0",
						"shadow": "none"
					}
				}
			},
			"core/group": {
				"custom": {
					"cardFooter": {
						"background": "none",
						"borderColor": "var:preset|color|neutral-100",
						"borderWidth": "var(--wp--custom--global--border-width) 0 0"
					}
				}
			},
			"core/navigation-submenu": {
				"custom": {
					"color": "var:preset|color|neutral-50",
					"background": "var:preset|color|neutral-950",
					"minWidth": "18rem"
				}
			},
			"core/paragraph": {
				"custom": {
					"dropCap": {
						"fontFamily": "var:preset|font-family|primary"
					}
				}
			},
			"core/post-comments-form": {
				"custom": {
					"commentNotes": {
						"color": "var:preset|color|neutral-500",
						"fontSize": "var:preset|font-size|xs",
						"lineHeight": "var:preset|line-height|xs"
					}
				}
			},
			"core/post-terms": {
				"custom": {
					"fill": {
						"hoverColor": "var:preset|color|base",
						"hoverBackground": "var:preset|color|contrast"
					}
				}
			},
			"core/social-links": {
				"custom": {
					"outline": {
						"borderColor": "var:preset|color|neutral-300",
						"borderWidth": "1px"
					}
				}
			}
		},
		"custom": {
			"variation": "extant",
			"elements": {
				"summary": {
					"fontSize": "var:preset|font-size|sm",
					"lineHeight": "var:custom|line-height|sm"
				}
			},
			"global": {
				"borderColor": "var:preset|color|neutral-100",
				"borderWidth": "1px",
				"borderRadius": "0",
				"shadow": "var:preset|shadow|xs"
			}
		},
		"color": {
			"palette": [
				{
					"slug": "base",
					"color": "#ffffff",
					"name": "Base"
				},
				{
					"slug": "contrast",
					"color": "#000000",
					"name": "Contrast"
				},
				{
					"slug": "primary-950",
					"color": "#460b18",
					"name": "Primary: 950"
				},
				{
					"slug": "primary-900",
					"color": "#7f1c36",
					"name": "Primary: 900"
				},
				{
					"slug": "primary-700",
					"color": "#b0203b",
					"name": "Primary: 700"
				},
				{
					"slug": "primary-500",
					"color": "#e64d60",
					"name": "Primary: 500"
				},
				{
					"slug": "primary-300",
					"color": "#f6abb0",
					"name": "Primary: 300"
				},
				{
					"slug": "primary-100",
					"color": "#fde6e7",
					"name": "Primary: 100"
				},
				{
					"slug": "primary-50",
					"color": "#fef2f2",
					"name": "Primary: 50"
				},
				{
					"slug": "neutral-950",
					"color": "#292929",
					"name": "Neutral: 950"
				},
				{
					"slug": "neutral-900",
					"color": "#3d3d3d",
					"name": "Neutral: 900"
				},
				{
					"slug": "neutral-700",
					"color": "#525252",
					"name": "Neutral: 700"
				},
				{
					"slug": "neutral-500",
					"color": "#7c7c7c",
					"name": "Neutral: 500"
				},
				{
					"slug": "neutral-300",
					"color": "#bdbdbd",
					"name": "Neutral: 300"
				},
				{
					"slug": "neutral-100",
					"color": "#f2f2f2",
					"name": "Neutral: 100"
				},
				{
					"slug": "neutral-50",
					"color": "#f8f8f8",
					"name": "Neutral: 50"
				}
			]
		},
		"typography": {
			"fontFamilies": [
				{
					"fontFamily": "'Roboto Slab', Rockwell, 'Rockwell Nova', 'Roboto Slab', 'DejaVu Serif', 'Sitka Small', serif",
					"slug": "primary",
					"name": "Primary (Roboto Slab)",
					"fontFace": [
						{
							"fontFamily": "Roboto Slab",
							"fontWeight": "100 900",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"src": [
								"file:./public/fonts/roboto/roboto-slab.woff2"
							]
						}
					]
				},
				{
					"fontFamily": "'Roboto Flex', ui-sans-serif, sans-serif",
					"slug": "secondary",
					"name": "Secondary (Roboto Flex)",
					"fontFace": [
						{
							"fontFamily": "Roboto Flex",
							"fontWeight": "100 1000",
							"fontStyle": "normal",
							"fontStretch": "25% 151%",
							"src": [
								"file:./public/fonts/roboto/roboto-flex.woff2"
							]
						}
					]
				},
				{
					"fontFamily": "ui-monospace, 'Fira Code', 'Cascadia Code', 'Source Code Pro', Menlo, Consolas, 'DejaVu Sans Mono', monospace",
					"slug": "mono",
					"name": "Monospace"
				}
			]
		}
	},
	"styles": {
		"elements": {
			"button": {
				"border": {
					"radius": "0"
				},
				"typography": {
					"fontWeight": "400"
				},
				":hover": {
					"color": {
						"background": "var:preset|color|contrast",
						"text": "var:preset|color|base"
					}
				},
				":focus": {
					"color": {
						"background": "var:preset|color|contrast",
						"text": "var:preset|color|base"
					}
				}
			},
			"caption": {
				"typography": {
					"fontFamily": "var:preset|font-family|primary",
					"fontStyle": "normal"
				}
			},
			"cite": {
				"typography": {
					"fontFamily": "var:preset|font-family|primary",
					"fontStyle": "normal"
				}
			},
			"heading": {
				"typography": {
					"fontWeight": "800"
				}
			},
			"h1": {
				"typography": {
					"fontSize": "var:preset|font-size|3-xl",
					"lineHeight": "var:custom|line-height|3-xl"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var:preset|font-size|2-xl",
					"lineHeight": "var:custom|line-height|2-xl"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var:preset|font-size|xl",
					"lineHeight": "var:custom|line-height|xl"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var:preset|font-size|lg",
					"lineHeight": "var:custom|line-height|lg"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var:preset|font-size|md",
					"lineHeight": "var:custom|line-height|md"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var:preset|font-size|sm",
					"lineHeight": "var:custom|line-height|sm"
				}
			}
		},
		"blocks": {
			"core/avatar": {
				"border": {
					"color": "var:custom|global|border-color",
					"style": "var:custom|global|border-style",
					"width": "1px"
				},
				"css": "& img {\n\tpadding: 2px;\n}"
			},
			"core/button": {
				"variations": {
					"outline": {
						"border": {
							"color": "var:preset|color|neutral-300"
						}
					}
				}
			},
			"core/comment-author-name": {
				"elements": {
					"link": {
						"color": {
							"text": "var:preset|color|neutral-900"
						},
						"typography": {
							"textDecoration": "none"
						},
						":focus": {
							"color": {
								"text": {"ref": "styles.elements.link.color.text"}
							},
							"typography": {
								"textDecoration": "underline"
							}
						},
						":hover": {
							"color": {
								"text": {"ref": "styles.elements.link.color.text"}
							},
							"typography": {
								"textDecoration": "underline"
							}
						}
					}
				}
			},
			"core/comment-date": {
				"elements": {
					"link": {
						"color": {
							"text": "var:preset|color|neutral-900"
						},
						"typography": {
							"textDecoration": "none"
						},
						":focus": {
							"color": {
								"text": {"ref": "styles.elements.link.color.text"}
							},
							"typography": {
								"textDecoration": "underline"
							}
						},
						":hover": {
							"color": {
								"text": {"ref": "styles.elements.link.color.text"}
							},
							"typography": {
								"textDecoration": "underline"
							}
						}
					}
				}
			},
			"core/comment-edit-link": {
				"elements": {
					"link": {
						"color": {
							"text": "var:preset|color|neutral-900"
						},
						"typography": {
							"textDecoration": "none"
						},
						":focus": {
							"color": {
								"text": {"ref": "styles.elements.link.color.text"}
							},
							"typography": {
								"textDecoration": "underline"
							}
						},
						":hover": {
							"color": {
								"text": {"ref": "styles.elements.link.color.text"}
							},
							"typography": {
								"textDecoration": "underline"
							}
						}
					}
				}
			},
			"core/comment-reply-link": {
				"elements": {
					"link": {
						"color": {
							"text": "var:preset|color|neutral-900"
						},
						"typography": {
							"textDecoration": "none"
						},
						":focus": {
							"color": {
								"text": {"ref": "styles.elements.link.color.text"}
							},
							"typography": {
								"textDecoration": "underline"
							}
						},
						":hover": {
							"color": {
								"text": {"ref": "styles.elements.link.color.text"}
							},
							"typography": {
								"textDecoration": "underline"
							}
						}
					}
				}
			},
			"core/comment-template": {
				"border": {
					"color": "var:custom|global|border-color",
					"style": "var:custom|global|border-style",
					"width": "var:custom|global|border-width",
					"radius": "var:custom|global|border-radius"
				},
				"spacing": {
					"margin": {
						"top": "0"
					},
					"padding": {
						"top": "var:preset|spacing|plus-3",
						"bottom": "var:preset|spacing|plus-3",
						"left": "var:preset|spacing|plus-3",
						"right": "var:preset|spacing|plus-3"
					}
				}
			},
			"core/comments-title": {
				"border": {
					"color": "var:custom|global|border-color",
					"style": "var:custom|global|border-style",
					"width": "var:custom|global|border-width",
					"radius": "var:custom|global|border-radius",
					"bottom": {
						"width": "0"
					}
				},
				"spacing": {
					"margin": {
						"bottom": "0"
					},
					"padding": {
						"top": "var:preset|spacing|base",
						"bottom": "var:preset|spacing|base",
						"left": "var:preset|spacing|plus-3",
						"right": "var:preset|spacing|plus-3"
					}
				},
				"typography": {
					"fontSize": "var:preset|font-size|lg",
					"lineHeight": "var:custom|line-height|lg"
				}
			},
			"core/navigation-submenu": {
				"border": {
					"width": "0",
					"radius": "var:custom|global|border-radius"
				},
				"color": {
					"text": "var:preset|color|neutral-50",
					"background": "var:preset|color|neutral-950"
				},
				"shadow": "var:preset|shadow|md",
				"spacing": {
					"padding": {
						"top": "var:preset|spacing|base",
						"bottom": "var:preset|spacing|base",
						"left": "var:preset|spacing|base",
						"right": "var:preset|spacing|base"
					}
				},
				"typography": {
					"fontSize": "inherit"
				},
				"elements": {
					"link": {
						"spacing": {
							"padding": {
								"top": "var:preset|spacing|minus-2",
								"bottom": "var:preset|spacing|minus-2",
								"left": "var:preset|spacing|base",
								"right": "var:preset|spacing|base"
							}
						},
						":focus": {
							"color": {
								"text": "inherit"
							}
						},
						":hover": {
							"color": {
								"text": "inherit"
							}
						}
					}
				},
				"css": "--wp--custom--submenu-item--hover-background: var(--wp--preset--color--neutral-900);"
			},
			"core/post-author-name": {
				"elements": {
					"link": {
						"color": {
							"text": "var:preset|color|neutral-900"
						},
						"typography": {
							"textDecoration": "none"
						},
						":focus": {
							"color": {
								"text": {"ref": "styles.elements.link.color.text"}
							},
							"typography": {
								"textDecoration": "underline"
							}
						},
						":hover": {
							"color": {
								"text": {"ref": "styles.elements.link.color.text"}
							},
							"typography": {
								"textDecoration": "underline"
							}
						}
					}
				}
			},
			"core/post-date": {
				"elements": {
					"link": {
						"color": {
							"text": "var:preset|color|neutral-900"
						},
						"typography": {
							"textDecoration": "none"
						},
						":focus": {
							"color": {
								"text": {"ref": "styles.elements.link.color.text"}
							},
							"typography": {
								"textDecoration": "underline"
							}
						},
						":hover": {
							"color": {
								"text": {"ref": "styles.elements.link.color.text"}
							},
							"typography": {
								"textDecoration": "underline"
							}
						}
					}
				}
			},
			"core/post-comments-link": {
				"elements": {
					"link": {
						"color": {
							"text": "var:preset|color|neutral-900"
						},
						"typography": {
							"textDecoration": "none"
						},
						":focus": {
							"color": {
								"text": {"ref": "styles.elements.link.color.text"}
							},
							"typography": {
								"textDecoration": "underline"
							}
						},
						":hover": {
							"color": {
								"text": {"ref": "styles.elements.link.color.text"}
							},
							"typography": {
								"textDecoration": "underline"
							}
						}
					}
				}
			},
			"core/post-comments-form": {
				"border": {
					"color": "var:custom|global|border-color",
					"style": "var:custom|global|border-style",
					"width": "var:custom|global|border-width"
				},
				"elements": {
					"heading": {
						"border": {
							"bottom": {
								"color": "var:custom|global|border-color",
								"style": "var:custom|global|border-style",
								"width": "var:custom|global|border-width"
							}
						},
						"spacing": {
							"padding": {
								"top": "var:preset|spacing|base",
								"bottom": "var:preset|spacing|base",
								"left": "var:preset|spacing|plus-3",
								"right": "var:preset|spacing|plus-3"
							}
						}
					},
					"h3": {
						"typography": {
							"fontSize": "var:preset|font-size|lg",
							"lineHeight": "var:custom|line-height|lg"
						}
					}
				},
				"css": "& > form {\n\tmargin: 0;\n\tpadding: var(--wp--preset--spacing--plus-3);\n}"
			},
			"core/post-terms": {
				"elements": {
					"link": {
						"color": {
							"text": "var:preset|color|neutral-900"
						},
						"typography": {
							"textDecoration": "none"
						},
						":focus": {
							"color": {
								"text": {"ref": "styles.elements.link.color.text"}
							},
							"typography": {
								"textDecoration": "underline"
							}
						},
						":hover": {
							"color": {
								"text": {"ref": "styles.elements.link.color.text"}
							},
							"typography": {
								"textDecoration": "underline"
							}
						}
					}
				}
			},
			"core/pullquote": {
				"typography": {
					"fontFamily": "var:preset|font-family|primary"
				}
			},
			"core/query-pagination": {
				"typography": {
					"fontSize": "var:preset|font-size|sm",
					"lineHeight": "var:custom|line-height|sm",
					"fontWeight": "500"
				}
			},
			"core/quote": {
				"typography": {
					"fontFamily": "var:preset|font-family|primary"
				}
			},
			"core/social-link": {
				"color": {
					"text": "white",
					"background": "purple"
				}
			},
			"x3p0/breadcrumbs": {
				"typography": {
					"fontSize": "var:preset|font-size|xs",
					"lineHeight": "var:custom|line-height|xs"
				}
			},
			"variations": {
				"box": {
					"blockTypes": [ "core/group" ],
					"border": {
						"color": "var:custom|global|border-color"
					},
					"shadow": "var:preset|shadow|md"
				},
				"card": {
					"blockTypes": [ "core/group" ],
					"color": {
						"background": "transparent"
					},
					"shadow": "var:preset|shadow|md"
				},
				"post-byline": {
					"blockTypes": [ "core/group" ],
					"color": {
						"text": "var:preset|color|neutral-500"
					}
				},
				"post-meta": {
					"blockTypes": [ "core/group" ],
					"color": {
						"text": "var:preset|color|neutral-500"
					}
				},
				"site-footer": {
					"blockTypes": [
						"core/columns",
						"core/group"
					],
					"border": {
						"top": {
							"color": "var:preset|color|neutral-100",
							"width": "1px"
						}
					},
					"color": {
						"background": "var:preset|color|neutral-50"
					}
				}
			}
		}
	}
}
